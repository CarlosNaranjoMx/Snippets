{
	//se deben eliminar primero las relaciones

	//**************([0]Start)**************//
	//**************([1]Comentarios)**************//
	//**************([2]oracle)**************//
	//**************([3]Oracle consultas)**************//
	//**************([4]llaves)**************//
	//**************([5]usuarios)**************//

	//**************([0]Start)**************//
	"start": {
		"prefix": "start", 
		"body": [
		"alter session set \"_ORACLE_SCRIPT\"=true;"
		]
	},

	//**************([1]Comentarios)**************//
	"Separador": {
		"prefix": "separador", 
		"body": [
		"----------------($1)----------------"
		]
	}

	//**************([2]oracle)**************//
	,"Number": {
		"prefix": "number", 
		"body": [
		"NUMBER(10)"
		]
	}
	,"char": {
		"prefix": "varchar", 
		"body": [
		"VARCHAR(250)"
		]
	}
	,"table": {
		"prefix": "table", 
		"body": [
		"CREATE TABLE $1 ( \n $2 \n);"
		]
	}
	,"default": {
		"prefix": "default", 
		"body": [
		"id_$1 NUMBER GENERATED BY DEFAULT AS IDENTITY,\nombre_$1 VARCHAR(250)"
		]
	}
	,"sf": {
		"prefix": "sf", 
		"body": [
		"SELECT $1\nFROM $2;"
		]
	}
	,"sfgh": {
		"prefix": "sfgh", 
		"body": [
		"SELECT $1\nFROM $2\nGROUP BY $3\nHAVING $4"
		]
	}
	,"drop": {
		"prefix": "drop", 
		"body": [
		"DROP TABLE $1;"
		]
	}
	,"pk": {
		"prefix": "pk", 
		"body": [
		"ALTER TABLE $1\nADD CONSTRAINT pk_$2\n PRIMARY KEY ($3);"
		]
	}
	,"fk": {
		"prefix": "fk", 
		"body": [
		"ALTER TABLE $1\nADD CONSTRAINT $2\nFOREIGN KEY(id_$3)\nREFERENCES ${4:tabla}(id_$3;)"
		]
	}

	//**************([3]Oracle consultas)**************//
	,"INSERT": {
		"prefix": "INSERT", 
		"body": [
		"INSERT INTO ${1:table}\n($2)\nVALUES\n($3);"
		]
	}
	,"update": {
		"prefix": "update", 
		"body": [
		"UPDATE ${1:TABLA}\nSET $2;"
		]
	}
	//**************([4]llaves)**************//
	,"primaria": {
		"prefix": "primaria", 
		"body": [
		"ALTER TABLE $1 \n ADD CONSTRAINT pk_$1 \n PRIMARY KEY (id_$1);"
		]
	}
	,"foranea": {
		"prefix": "foranea", 
		"body": [
		"ALTER TABLE $1\nADD CONSTRAINT fk1_$2\nFOREIGN KEY ($3)\nREFERENCES Medico($4);"
		]
	}
	//**************([5]usuarios)**************//
	,"usuario": {
		"prefix": "usuario", 
		"body": [
		"grant all privileges to userName identified by password;"
		]
	}

	
}