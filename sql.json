{
	//**************(Comentarios)**************//
	"Separador": {
		"prefix": "separador", 
		"body": [
		"----------------($1)----------------"
		]
	}

	//**************(oracle)**************//
	,"Number": {
		"prefix": "number", 
		"body": [
		"NUMBER(10)"
		]
	}
	,"char": {
		"prefix": "varchar", 
		"body": [
		"VARCHAR(250)"
		]
	}
	,"table": {
		"prefix": "table", 
		"body": [
		"CREATE TABLE $1 ( \n $2 \n);"
		]
	}
	,"default": {
		"prefix": "default", 
		"body": [
		"id_$1 NUMBER GENERATED BY DEFAULT AS IDENTITY,\nombre_$1 VARCHAR(250)"
		]
	}
	,"sf": {
		"prefix": "sf", 
		"body": [
		"SELECT $1\nFROM $2;"
		]
	}
	,"sfgh": {
		"prefix": "sfgh", 
		"body": [
		"SELECT $1\nFROM $2\nGROUP BY $3\nHAVING $4"
		]
	}
	,"INSERT": {
		"prefix": "INSERT", 
		"body": [
		"INSERT INTO ${1:table}\n($2)\nVALUES\n($3);"
		]
	}
	,"update": {
		"prefix": "update", 
		"body": [
		"UPDATE ${1:TABLA}\nSET $2;"
		]
	}

	
}