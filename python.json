{
	//**************(Diccionarios)**************//
	// condicion booleana
	// lectura y escritura con []
	"Diccionario": {
		"prefix": "in",
		"body": [
		"${1:objeto} in ${2:diccionario}"
		]
	}
	//**************(Comments)**************//
	,"Function comment": {
		"prefix": "fcomment",
		"body": [
		"\"\"\"\n$1\n\"\"\""
		]
	}
	//**************(Prints)**************//
	// impresion con formato
	,"fprint": {
		"prefix": "fprint",
		"body": [
		"print('${1:cadena}{}'.format(${2:variable}))"
		]
	}
