{
	//**************(Indice)**************//
	//**************([1]Basics)**************//
	//**************([2]Notes)**************//


	//**************([1]Basics)**************//
	"Block": {
		"prefix": "block", 
		"body": [
		"~~~\n$1\n~~~"
		]
	}
	,"code": {
		"prefix": "code", 
		"body": [
		"`$1`"
		]
	}
	,"image": {
		"prefix": "image", 
		"body": [
		"![](${1:image})"
		]
	}
	,"sfont": {
		"prefix": "sfont", 
		"body": [
		"_$1_"
		]
	}
	//**************([2]Notes)**************//
	,"startnote": {
		"prefix": "startnote", 
		"body": [
		"# ${1:titulo}\n## Indice\n "
		]
	}
	//**************([3])**************//
	,"indice": {
		"prefix": "indice", 
		"body": [
		"[${1:titulo}](#${1:etiqueta})"
		]
	}
}