	,"pesos": {
		"prefix": "pesos", 
		"body": [
		"${$1:$2}"
		]
	}
	,"salto": {
		"prefix": "salto", 
		"body": [
		"\\n$$1\\n"
		]
	}
	,"llsalto": {
		"prefix": "llsalto", 
		"body": [
		"{\\n$$1\\n}"
		]
	}
	,"4barras": {
		"prefix": "4barras", 
		"body": [
		"\\\\\\\\"
		]
	}
	//***************(Separadores)***************//
	,"separador": {
		"prefix": "separador", 
		"body": [
		"//***$2($1)$2***$3//"
		]
	}
	,"Fin separador": {
		//final separador
		"prefix": "fseparador", 
		"body": [
		"//**********************************$1//"
		]
	}
	//**************(Opcionales)**************//
	,"Opcional": {
		"prefix": "opcional", 
		"body": [
		"${1:pesos}{${2:numero}:$3}"
		]
	}
