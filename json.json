{
	//***************(Snippet de Snippets)***************//
	"Snippet": {
		"prefix": "script",
		"body": [
			",\"${1:Nombre}\": {",
			"	\"prefix\": \"$2\", ",
			"	\"body\": [",
			"	\"$3\""
			"	]",
			"}"
		]
	}

	,"Snippet2": {
		//init script
		"prefix": "iscript", 
		"body": [
			"\"${1:Nombre}\": {",
				"	\"prefix\": \"$2\", ",
				"	\"body\": [",
				"	\"$3\""
				"	]",
				"}"
		]
	}
	//***************(Separadores)***************//
	,"comentario separador": {
		"prefix": "separador", 
		"body": [
		"//***************($1)***************//"
		]
	}
	,"Fin separador": {
		//final separador
		"prefix": "fseparador", 
		"body": [
		"//**********************************$1//"
		]
	}
	//**************(Opcionales)**************//
	,"Opcional": {
		"prefix": "opcional", 
		"body": [
		"${1:pesos}{${2:numero}:$3}"
		]
	}
}